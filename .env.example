# Benchwarmer Analytics Environment Variables
# Copy this file to .env and update values as needed

# PostgreSQL Configuration (for Docker Compose)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Application Database Connection
# Used by the .NET API when running outside Docker
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=benchwarmer
DATABASE_USER=benchwarmer
DATABASE_PASSWORD=benchwarmer

# Hangfire Database Connection
HANGFIRE_DATABASE_NAME=hangfire
